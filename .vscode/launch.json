{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) 启动调试",
            "type": "cppdbg",
            "request": "launch",
    
            "program": "${workspaceFolder}/build/resnet18_thread", 
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}